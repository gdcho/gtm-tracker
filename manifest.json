{
  "manifest_version": 3,
  "name": "CTM Logger",
  "version": "1.0",
  "description": "Finds GTM CTM tags and logs them",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["injectedScript.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
